pipeline{
	agent any	//不分配节点 stage如果不指定 会报错 none 
	stages{
		stage("empty test"){
			//agent {label "JenkinsTest"}
			steps{
				bat label:'',script:" echo helloworld"
				//echo "helloworld1"	//这里直接执行也是可以的
				
				sleep(5)
			}
		}
	}
	post{
		always{
			bat label:'',script:"echo always show this msg"
		}
		aborted{
			bat label:'',script:" echo some stage aborted"
		}
		failure{
			bat label:'',script:" echo some stage failure"
		}
		success{
			bat label:'',script:" echo all success"
		}
	}
	
}