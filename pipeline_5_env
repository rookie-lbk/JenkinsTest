pipeline{
	agent any	//不分配节点 stage如果不指定 会报错 none 
	
	environment{
		TEST=" TOP Set Evn"
	}
	
	stages{
	
		
	
		stage("echo top Evn"){
			environment{
				INNER=" INNER Set Evn"
			}
			steps{
				bat label:'',script:"echo ${TEST}"
				bat label:'',script:"echo ${INNER}"
			}
		}
		
		stage("echo stages Evn"){

			steps{
				bat label:'',script:""
				//bat label:'',script:"echo ${INNER}"
			}
		}
		
	}
	
}